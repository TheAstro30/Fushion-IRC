<?xml version="1.0" encoding="utf-8"?>
<script xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="dcc-chat">
  <data>Whois:whois $nick</data>
  <data>Query:query $nick</data>
  <data>-</data>
  <data>Ignore:ignore $nick</data>
  <data>Unignore:ignore -r $nick</data>
  <data>-</data>
  <data>DCC</data>
  <data>.Send:dcc send $nick</data>
  <data>CTCP</data>
  <data>.Ping:ctcp $nick ping</data>
  <data>.Version:ctcp $nick version</data>
  <data>.Time:ctcp $nick time</data>
  <data>.Finger:ctcp $nick finger</data>
  <data>-</data>
  <data>Popups</data>
  <data>.Slap:me slaps $nick around a bit with a dirty sock!</data>
  <data>.Random Slap:$read(scripts/randslap.txt)</data>
</script>